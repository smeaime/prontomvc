                    If Not oWB Is Nothing Then oWB.Close(False)
                    NAR(oWB)
                    oWBs.Close()
                    NAR(oWBs)
                    'quit and dispose app
                    oXL.Quit()
                    NAR(oXL)
                    'VERY IMPORTANT
                    GC.Collect()

