            function panelessetup() {


                estiloArbol();

                if (!(pageLayout.state.east.isHidden || pageLayout.state.east.isClosed)) {
                    $("#btnConmutarPanelDerecho").hide();
                }

                if (!(pageLayout.state.west.isHidden || pageLayout.state.west.isClosed)) {
