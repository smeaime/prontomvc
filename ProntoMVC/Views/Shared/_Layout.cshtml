            function ReestableceScroll() {
                //////////////////////////////////////////////////////////
                //////////////////////////////////////////////////////////
                //////////////////////////////////////////////////////////
                //    http: //browse-tutorials.com/tutorial/save-window-scroll-position-jquery
                $.cookies.get('myCookie')

                //set window scroll position if cookie is set
                //window.scroll(0, $.cookies.get('myCookie'));
                $("#panelOeste").scrollTop($.cookies.get('myCookie'));

                //unset cookie after setting scroll position
                $.cookies.del('myCookie');

                //make this class objects keep page scroll position
                $('.link-class').each(function () {
                    $(this).click(function () {
                        $.cookies.set('myCookie', getPageScroll());
                    });
                });

                $('a[href!=""]').each(function () {
                    $(this).click(function () {
                        $.cookies.set('myCookie', getPageScroll());
                    });
                });

                $(':input').each(function () {
                    $(this).click(function () {
                        $.cookies.set('myCookie', getPageScroll());
                    });
                });
                //////////////////////////////////////////////////////////
                //////////////////////////////////////////////////////////
                //////////////////////////////////////////////////////////

            }


            function addCommas(nStr) {
                nStr += '';
                x = nStr.split('.');
                x1 = x[0];
                x2 = x.length > 1 ? '.' + x[1] : '';
                var rgx = /(\d+)(\d{3})/;
                while (rgx.test(x1)) {
                    x1 = x1.replace(rgx, '$1' + ',' + '$2');
                }
                return x1 + x2;
            }



            //    http: //browse-tutorials.com/tutorial/save-window-scroll-position-jquery
            //this function simply gets the window scroll position, works in all browsers
            function getPageScroll() {

                var yScroll;

                return $("#panelOeste").scrollTop();

                if (self.pageYOffset) {
                    yScroll = self.pageYOffset;
                } else if (document.documentElement && document.documentElement.scrollTop) {
                    yScroll = document.documentElement.scrollTop;
                } else if (document.body) {
                    yScroll = document.body.scrollTop;
                }
                return yScroll
            }







        </script>

