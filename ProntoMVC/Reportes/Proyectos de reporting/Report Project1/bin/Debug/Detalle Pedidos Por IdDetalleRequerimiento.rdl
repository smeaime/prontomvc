  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@IdDetalleRequerimiento">
            <Value>=Parameters!IdDetalleRequerimiento.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT 
 d.IdDetallePedido as [IdDetallePedido], 
 Pedidos.NumeroPedido as [NumeroPedido],
 d.NumeroItem as [Item_PE],
 Pedidos.FechaPedido as [FechaPedido],
 Proveedores.RazonSocial as [Proveedor],
 E1.Nombre as [Comprador],
 d.FechaEntrega as [FechaEntrega],
 d.Cantidad as [Cant_PE],
 Unidades.Abreviatura as [Unidad_PE],
 Monedas.Abreviatura as [Moneda],
 d.Precio as [PrecioUnitario],
 (d.Cantidad*d.Precio) as [ImporteItem]
FROM DetallePedidos d
LEFT OUTER JOIN Pedidos ON Pedidos.IdPedido = d.IdPedido
LEFT OUTER JOIN Proveedores ON Pedidos.IdProveedor = Proveedores.IdProveedor
LEFT OUTER JOIN Empleados E1 ON E1.IdEmpleado = Pedidos.IdComprador
LEFT OUTER JOIN Empleados E2 ON E2.IdEmpleado = Pedidos.Aprobo
LEFT OUTER JOIN Unidades ON Unidades.IdUnidad = d.IdUnidad
LEFT OUTER JOIN Monedas ON Monedas.IdMoneda = Pedidos.IdMoneda
WHERE IsNull(Pedidos.Cumplido,'NO')&lt;&gt;'AN' and IsNull(d.Cumplido,'NO')&lt;&gt;'AN' and d.IdDetalleRequerimiento=@IdDetalleRequerimiento</CommandText>
      </Query>
