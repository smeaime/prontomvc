Attribute VB_Name = "frmRecibosOtrosConceptos"
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = False
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = False
Dim mRecibo As ComPronto.Recibo
Attribute mRecibo.VB_VarHelpID = -1
Dim WithEvents oBind As BindingCollection
Attribute oBind.VB_VarHelpID = -1
Dim mvarId As Long

Private Sub Check2_Click(Index As Integer)

   If Check2(Index).Value = 0 Then
      Dim mIdCuenta As Long
      mRecibo.Registro.Fields(DataCombo3(Index).DataField).Value = Null
      txtCodigoCuenta(Index).Enabled = True
      mIdCuenta = 0
      With DataCombo1(Index)
         If IsNumeric(.BoundText) Then mIdCuenta = .BoundText
         Set .RowSource = Aplicacion.Cuentas.TraerLista
         mRecibo.Registro.Fields(.DataField).Value = mIdCuenta
         .Enabled = True
      End With
      With DataCombo4(Index)
         .BoundText = 0
         .Enabled = True
      End With
   End If
   
End Sub

Private Sub cmd_Click(Index As Integer)

   Select Case Index
   
      Case 0
      
         Dim dc As DataCombo
         Dim dtp As DTPicker
         Dim est As EnumAcciones
         Dim i As Integer
         Dim mAux1 As String
         
         With mRecibo.Registro
            For i = 1 To 10
               If IsNull(.Fields("Otros" & i).Value) Then
                  .Fields("Otros" & i).Value = 0
               End If
            Next
         End With
      
         Unload Me
   
      Case 1
      
         Unload Me
   
   End Select

End Sub

Public Property Let Id(ByVal vnewvalue As Long)

   Dim oControl As Control
   
   mvarId = vnewvalue
   
   Set oBind = New BindingCollection
   With oBind
      Set .DataSource = mRecibo
      For Each oControl In Me.Controls
         If TypeOf oControl Is CommandButton Then
         ElseIf TypeOf oControl Is Label Then
            If Len(oControl.DataField) Then .Add oControl, "Caption", oControl.DataField
         ElseIf TypeOf oControl Is RichTextBox Then
            If Len(oControl.DataField) Then .Add oControl, "textRTF", oControl.DataField
         ElseIf TypeOf oControl Is DataCombo Then
            If Len(oControl.DataField) Then Set oControl.DataSource = mRecibo
            If Len(oControl.Tag) Then
               Set oControl.RowSource = Aplicacion.CargarLista(oControl.Tag)
            End If
         Else
            On Error Resume Next
            Set oControl.DataSource = mRecibo
         End If
      Next
   End With
   
End Property

Public Property Set Recibo(ByRef vnewvalue As ComPronto.Recibo)

   Set mRecibo = vnewvalue

End Property

Private Sub DataCombo1_Change(Index As Integer)

   If IsNumeric(DataCombo1(Index).BoundText) Then
      Dim oRs As ADOR.Recordset
      Set oRs = Aplicacion.Cuentas.TraerFiltrado("_PorId", DataCombo1(Index).BoundText)
      If oRs.RecordCount > 0 Then
         With mRecibo.Registro
            If Not IsNull(oRs.Fields("EsCajaBanco").Value) And _
                  (oRs.Fields("EsCajaBanco").Value = "CA" Or _
                   oRs.Fields("EsCajaBanco").Value = "BA") Then
               MsgBox "No puede ingresar cuentas de caja o banco por otros," & vbCrLf & _
                     "registrelo en el cuadro de valores", vbExclamation
               .Fields(DataCombo1(Index).DataField).Value = Null
               txtCodigoCuenta(Index).Text = ""
            Else
               .Fields(DataCombo1(Index).DataField).Value = DataCombo1(Index).BoundText
               txtCodigoCuenta(Index).Text = oRs.Fields("Codigo").Value
            End If
         End With
      End If
      oRs.Close
      Set oRs = Nothing
   End If

End Sub

