Private Sub txtCodigoCuenta_Change(Index As Integer)

   If IsNumeric(txtCodigoCuenta(Index).Text) Then
      On Error GoTo SalidaConError
      Dim oRs As ADOR.Recordset
      Set oRs = Aplicacion.Cuentas.TraerFiltrado("Cod", txtCodigoCuenta(Index).Text)
      If oRs.RecordCount > 0 Then
         mRecibo.Registro.Fields("IdCuenta" & (Index + 1)).Value = oRs.Fields(0).Value
      Else
         mRecibo.Registro.Fields("IdCuenta" & (Index + 1)).Value = Null
      End If
      oRs.Close
      Set oRs = Nothing
      Exit Sub
SalidaConError:
      Set oRs = Nothing
      With mRecibo.Registro
         .Fields(DataCombo1(Index).DataField).Value = Null
      End With
   End If

End Sub

