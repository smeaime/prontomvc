Private Sub cmd_Click(Index As Integer)

   On Error GoTo Mal
   
   Select Case Index
      Case 0
         If Len(Trim(txtCodigoCliente.Text)) = 0 Then
            MsgBox "Falta completar el campo codigo!", vbCritical
            Exit Sub
         End If
         
         If Not IsNumeric(txtCodigoCliente.Text) Then
            MsgBox "El campo codigo debe ser numerico!", vbCritical
            Exit Sub
         End If
         
         If Not Option1.Value And Not Option2.Value And Not Option3.Value And Not Option4.Value Then
            MsgBox "Falta indicar el tipo de cliente!", vbCritical
            Exit Sub
         End If
         
         If Len(Trim(txtRazonSocial.Text)) = 0 Then
            MsgBox "Falta completar el campo razon social!", vbCritical
            Exit Sub
         End If
         
         If Len(Trim(txtDireccion.Text)) = 0 Then
            MsgBox "Falta completar el campo direccion!", vbCritical
            Exit Sub
         End If
         
         If txtCBU.Enabled And Len(Trim(txtCBU.Text)) = 0 Then
            MsgBox "Falta completar el campo CBU!", vbCritical
            Exit Sub
         End If
         
         Dim est As EnumAcciones
         Dim oControl As Control
         Dim dc As DataCombo
         Dim dtp As DTPicker
         Dim mCodigo As String
         Dim oRs As ADOR.Recordset
         Dim mNumeroCliente As Long
         Dim mAux1
   
         If (mvarId <= 0 Or BuscarClaveINI("Control estricto del CUIT") = "SI") And Len(Trim(CUIT1.Text)) > 0 Then
            Set oRs = Aplicacion.Clientes.TraerFiltrado("_PorCuit", CUIT1.Text)
            If oRs.RecordCount > 0 Then
               Do While Not oRs.EOF
                  If mvarId <> oRs.Fields(0).Value Then
                     MsgBox "El CUIT ya fue asignado al cliente " & oRs.Fields("RazonSocial").Value, vbExclamation
                     oRs.Close
                     Set oRs = Nothing
                     Exit Sub
                  End If
                  oRs.MoveNext
               Loop
            End If
            oRs.Close
         End If
         
         With origen.Registro
            For Each dtp In DTPicker1
               If dtp.Enabled Then
                  .Fields(dtp.DataField).Value = dtp.Value
               End If
            Next
            For Each dc In DataCombo1
               If Not IsNumeric(dc.BoundText) And dc.Enabled And dc.Visible And _
                     dc.Index <> 4 And dc.Index <> 5 And dc.Index <> 14 And _
                     Not (dc.Index = 10 And (Option8.Value Or Option11.Value)) Then
                  MsgBox "Falta completar el campo " & lblData(dc.Index).Caption, vbCritical
                  Exit Sub
               End If
               If IsNumeric(dc.BoundText) Then
                  .Fields(dc.DataField).Value = dc.BoundText
               End If
            Next
         
            For Each oControl In Me.Controls
               If TypeOf oControl Is DataCombo Then
                  If Len(oControl.BoundText) <> 0 And IsNumeric(oControl.BoundText) Then
                     .Fields(oControl.DataField).Value = oControl.BoundText
                  End If
               ElseIf TypeOf oControl Is DTPicker Then
                  If oControl.Enabled Then
                     .Fields(oControl.DataField).Value = oControl.Value
                  Else
                     If oControl.Index > 0 Then .Fields(oControl.DataField).Value = Null
                  End If
               ElseIf TypeOf oControl Is TextBox Then
                  If Len(oControl.DataField) > 0 Then
                     If Len(oControl.Text) = 0 Then
                        .Fields(oControl.DataField).Value = Null
                     Else
                        .Fields(oControl.DataField).Value = oControl.Text
                     End If
                  End If
               End If
            Next
            
            If Len(Trim(CUIT1.Text)) = 0 Then
               Set oRs = Aplicacion.TablasGenerales.TraerFiltrado("DescripcionIva", "_PorId", .Fields("IdCodigoIva").Value)
               If oRs.RecordCount > 0 Then
                  If Not IsNull(oRs.Fields("ExigirCUIT").Value) And oRs.Fields("ExigirCUIT").Value = "SI" Then
                     MsgBox "Debe ingresar el numero de CUIT para esta condicion", vbCritical
                     oRs.Close
                     Set oRs = Nothing
                     Exit Sub
                  End If
               End If
               oRs.Close
               Set oRs = Nothing
            End If
            
            If mvarId <= 0 And mvarNumerarClientes Then
               mAux1 = TraerValorParametro2("ProximoNumeroCliente")
               mNumeroCliente = IIf(IsNull(mAux1), 1, mAux1)
               .Fields("CodigoCliente").Value = mNumeroCliente
               GuardarValorParametro2 "ProximoNumeroCliente", "" & mNumeroCliente + 1
            End If
            
            mCodigo = ""
            If Option1.Value Then
               .Fields("TipoCliente").Value = 1
               mCodigo = "CL"
            ElseIf Option2.Value Then
               .Fields("TipoCliente").Value = 2
               mCodigo = "CE"
            ElseIf Option3.Value Then
               .Fields("TipoCliente").Value = 3
               mCodigo = "CV"
            ElseIf Option4.Value Then
               .Fields("TipoCliente").Value = 4
               mCodigo = "CO"
            End If
            If Len(IIf(IsNull(.Fields("Codigo").Value), "", .Fields("Codigo").Value)) = 0 Then
               .Fields("Codigo").Value = mCodigo & Format(txtCodigoCliente.Text, "0000")
            End If
            .Fields("Cuit").Value = CUIT1.Text
            If Option8.Value Then
               .Fields("IBCondicion").Value = 1
            ElseIf Option9.Value Then
               .Fields("IBCondicion").Value = 2
            ElseIf Option10.Value Then
               .Fields("IBCondicion").Value = 3
            ElseIf Option11.Value Then
               .Fields("IBCondicion").Value = 4
            End If
            .Fields("Confirmado").Value = "SI"
            .Fields("Observaciones").Value = rchObservaciones.Text
            If Check2.Value = 1 Then
               .Fields("EsAgenteRetencionIVA").Value = "SI"
            Else
               .Fields("EsAgenteRetencionIVA").Value = "NO"
            End If
            If mvarId < 0 Then
               .Fields("IdUsuarioIngreso").Value = glbIdUsuario
               .Fields("FechaIngreso").Value = Now
            Else
               .Fields("IdUsuarioModifico").Value = glbIdUsuario
               .Fields("FechaModifico").Value = Now
            End If
         End With
         
         Select Case origen.Guardar
            Case ComPronto.MisEstados.Correcto
            Case ComPronto.MisEstados.ModificadoPorOtro
               MsgBox "El Regsitro ha sido modificado"
            Case ComPronto.MisEstados.NoExiste
               MsgBox "El registro ha sido eliminado"
            Case ComPronto.MisEstados.ErrorDeDatos
               MsgBox "Error de ingreso de datos"
         End Select
      
         If mvarId < 0 Then
            est = alta
            mvarId = origen.Registro.Fields(0).Value
            Aplicacion.Tarea "Log_InsertarRegistro", _
                  Array("ALTA", mvarId, 0, Now, 0, "Tabla : Clientes", GetCompName(), glbNombreUsuario)
         Else
            est = Modificacion
            Aplicacion.Tarea "Log_InsertarRegistro", _
                  Array("MODIF", mvarId, 0, Now, 0, "Tabla : Clientes", GetCompName(), glbNombreUsuario)
         End If
            
         With actL2
            .ListaEditada = "ClientesResumen,ClientesDetalle,ClientesAConfirmar"
            .AccionRegistro = est
            .Disparador = mvarId
         End With
   
      Case 1
         Dim mBorra As Integer
         mBorra = MsgBox("Esta seguro de eliminar los datos definitivamente ?", vbYesNo, "Eliminar")
         If mBorra = vbNo Then Exit Sub
         
         origen.Eliminar
         
         est = baja
         Aplicacion.Tarea "Log_InsertarRegistro", _
               Array("ELIM", mvarId, 0, Now, 0, "Tabla : Clientes", GetCompName(), glbNombreUsuario)
            
         With actL2
            .ListaEditada = "ClientesResumen,ClientesDetalle,ClientesAConfirmar"
            .AccionRegistro = est
            .Disparador = mvarId
         End With
   
      Case 3
         Dim mSeguro As Integer
         mBorra = MsgBox("Esta seguro de copiar la direccion principal como direccion de entrega?", vbYesNo, "Copiar direccion")
         If mBorra = vbNo Then Exit Sub
   
         With origen.Registro
            .Fields("DireccionEntrega").Value = .Fields("Direccion").Value
            .Fields("IdLocalidadEntrega").Value = .Fields("IdLocalidad").Value
            .Fields("IdProvinciaEntrega").Value = .Fields("IdProvincia").Value
         End With
         Exit Sub
   
   End Select
   
   Unload Me

   Exit Sub

Mal:
   
   Dim mvarResp As Integer
   Select Case Err.Number
      Case -2147217900
         mvarResp = MsgBox("No puede borrar este registro porque se esta" & vbCrLf & "utilizando en otros archivos. Desea ver detalles?", vbYesNo + vbCritical)
         If mvarResp = vbYes Then
            MsgBox "Detalle del error : " & vbCrLf & Err.Number & " -> " & Err.Description
         End If
      Case Else
         MsgBox "Se ha producido un error al registrar los datos" & vbCrLf & Err.Number & " -> " & Err.Description, vbCritical
   End Select

End Sub

Public Property Let Id(ByVal vNewValue As Long)

   Dim oAp As ComPronto.Aplicacion
   Dim oRs As ADOR.Recordset
   Dim oControl As Control
   Dim mvarIdMonedaPesos As Integer, mvarIdMonedaDolar As Integer
   Dim ListaVacia1 As Boolean, ListaVacia2 As Boolean
   Dim mAux1
   
   mvarId = vNewValue
   
   ListaVacia1 = False
   ListaVacia2 = False
   
   mvarNumerarClientes = False
   If BuscarClaveINI("Numeracion automatica de clientes") = "SI" Then mvarNumerarClientes = True
   
   Set oAp = Aplicacion
   Set origen = oAp.Clientes.Item(vNewValue)
   Set oBind = New BindingCollection
   
   With oBind
      Set .DataSource = origen
      For Each oControl In Me.Controls
         If TypeOf oControl Is CommandButton Then
         ElseIf TypeOf oControl Is DbListView Then
            Select Case oControl.Name
               Case "Lista"
                  If vNewValue < 0 Then
                     Set oControl.DataSource = origen.DetClientes.TraerMascara
                     ListaVacia1 = True
                  Else
                     Set oRs = origen.DetClientes.TraerTodos
                     If oRs.RecordCount > 0 Then
                        Set oControl.DataSource = oRs
                        ListaVacia1 = False
                     Else
                        Set oControl.DataSource = origen.DetClientes.TraerMascara
                        ListaVacia1 = True
                     End If
                     oRs.Close
                  End If
               Case "ListaLugaresEntrega"
                  If vNewValue < 0 Then
                     Set oControl.DataSource = origen.DetClientesLugaresEntrega.TraerMascara
                     ListaVacia1 = True
                  Else
                     Set oRs = origen.DetClientesLugaresEntrega.TraerTodos
                     If oRs.RecordCount > 0 Then
                        Set oControl.DataSource = oRs
                        ListaVacia2 = False
                     Else
                        Set oControl.DataSource = origen.DetClientesLugaresEntrega.TraerMascara
                        ListaVacia2 = True
                     End If
                     oRs.Close
                  End If
            End Select
         ElseIf TypeOf oControl Is Label Then
            If Len(oControl.DataField) Then .Add oControl, "Caption", oControl.DataField
         ElseIf TypeOf oControl Is RichTextBox Then
            If Len(oControl.DataField) Then .Add oControl, "textRTF", oControl.DataField
         ElseIf TypeOf oControl Is DataCombo Then
            If Len(oControl.DataField) Then Set oControl.DataSource = origen
            If Len(oControl.Tag) Then
               Set oControl.RowSource = oAp.CargarLista(oControl.Tag)
            End If
         Else
            On Error Resume Next
            Set oControl.DataSource = origen
         End If
      Next
   End With
   
   If mvarId < 0 Then
      Set oRs = oAp.Parametros.Item(1).Registro
      With oRs
         mvarIdMonedaPesos = .Fields("IdMoneda").Value
         mvarIdMonedaDolar = .Fields("IdMonedaDolar").Value
      End With
      With origen.Registro
         .Fields("IdCuenta").Value = oRs.Fields("IdCuentaDeudoresVarios").Value
         .Fields("IdMoneda").Value = mvarIdMonedaPesos
      End With
      oRs.Close
      
      With origen.Registro
         If mvarNumerarClientes Then
            mAux1 = TraerValorParametro2("ProximoNumeroCliente")
            .Fields("CodigoCliente").Value = IIf(IsNull(mAux1), 1, mAux1)
         End If
      End With
      
      DTPicker1(0).Value = Date
      Option8.Value = True
      Check1(0).Value = 0
      Check1(1).Value = 0
   Else
      With origen.Registro
         If IsNull(.Fields("TipoCliente").Value) Then
            If mId(.Fields("Codigo").Value, 1, 2) = "CL" Then
               Option1.Value = True
            ElseIf mId(.Fields("Codigo").Value, 1, 2) = "CE" Then
               Option2.Value = True
            ElseIf mId(.Fields("Codigo").Value, 1, 2) = "CV" Then
               Option3.Value = True
            ElseIf mId(.Fields("Codigo").Value, 1, 2) = "CO" Then
               Option4.Value = True
            End If
         Else
            Select Case .Fields("TipoCliente").Value
               Case 1
                  Option1.Value = True
               Case 2
                  Option2.Value = True
               Case 3
                  Option3.Value = True
               Case 4
                  Option4.Value = True
            End Select
         End If
         CUIT1.Text = IIf(IsNull(.Fields("Cuit").Value), "", .Fields("Cuit").Value)
         If IsNull(.Fields("IBCondicion").Value) Then
            Option8.Value = True
         Else
            Select Case .Fields("IBCondicion").Value
               Case 1
                  Option8.Value = True
               Case 2
                  Option9.Value = True
               Case 3
                  Option10.Value = True
               Case 4
                  Option11.Value = True
            End Select
         End If
         If Not IsNull(.Fields("IdIBCondicionPorDefecto2").Value) Then
            Check1(0).Value = 1
         End If
         If Not IsNull(.Fields("IdIBCondicionPorDefecto3").Value) Then
            Check1(1).Value = 1
         End If
         If Not IsNull(.Fields("EsAgenteRetencionIVA").Value) And _
               .Fields("EsAgenteRetencionIVA").Value = "SI" Then
            Check2.Value = 1
         End If
         rchObservaciones.TextRTF = .Fields("Observaciones").Value
         If Not IsNull(.Fields("IdBancoDebito").Value) Then
            Check3.Value = 1
         End If
      End With
   End If
   
   If mvarNumerarClientes Then
      Option1.Value = True
      Frame1.Enabled = False
   End If
   
   If ListaVacia1 Then Lista.ListItems.Clear
   If ListaVacia2 Then ListaLugaresEntrega.ListItems.Clear
   
   Lista.Sorted = False
   ListaLugaresEntrega.Sorted = False
   
   cmd(1).Enabled = False
   cmd(0).Enabled = False
   If Me.NivelAcceso = Medio Then
      If mvarId <= 0 Then cmd(0).Enabled = True
   ElseIf Me.NivelAcceso = Alto Then
      cmd(0).Enabled = True
      If mvarId > 0 Then cmd(1).Enabled = True
   End If
   
   Set oRs = Nothing
   Set oAp = Nothing

End Property

