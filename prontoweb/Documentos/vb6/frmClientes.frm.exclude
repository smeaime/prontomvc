Public Sub GenerarCodigo()

   On Error Resume Next
   Dim mCodigo As String
   With origen.Registro
      mCodigo = ""
      If Option1.Value Then
         mCodigo = "CL"
      ElseIf Option2.Value Then
         mCodigo = "CE"
      ElseIf Option3.Value Then
         mCodigo = "CV"
      ElseIf Option4.Value Then
         mCodigo = "CO"
      End If
'      If Len(IIf(IsNull(.Fields("Codigo").Value), "", .Fields("Codigo").Value)) = 0 Then
         .Fields("Codigo").Value = mCodigo & Format(txtCodigoCliente.Text, "0000")
'      End If
   End With
   
End Sub

Public Function ValidarCliente() As Boolean

   ValidarCliente = True
   
   If Len(txtCodigoCliente.Text) <> 0 Then
      Dim oRs As ADOR.Recordset
      Dim mTipoCliente As Integer
      If Option1.Value Then
         mTipoCliente = 1
      ElseIf Option2.Value Then
         mTipoCliente = 2
      ElseIf Option3.Value Then
         mTipoCliente = 3
      ElseIf Option4.Value Then
         mTipoCliente = 4
      Else
         mTipoCliente = 0
      End If
      Set oRs = Aplicacion.Clientes.TraerFiltrado("_ValidarCodigo", Array(txtCodigoCliente.Text, mTipoCliente, mvarId))
      If oRs.RecordCount > 0 Then
         MsgBox "Codigo de cliente ya ingresado. Reingrese.", vbCritical
         ValidarCliente = False
      End If
      oRs.Close
      Set oRs = Nothing
   End If
   
End Function

Public Sub ActivarControlesIB(ByVal mActivar As Boolean)

   If Not mActivar Then
      With origen.Registro
         .Fields("IdIBCondicionPorDefecto").Value = Null
         .Fields("IdIBCondicionPorDefecto2").Value = Null
         .Fields("IdIBCondicionPorDefecto3").Value = Null
         .Fields("IBNumeroInscripcion").Value = Null
         .Fields("PorcentajeIBDirecto").Value = Null
         .Fields("FechaInicioVigenciaIBDirecto").Value = Null
         .Fields("FechaFinVigenciaIBDirecto").Value = Null
      End With
      Check1(0).Value = 0
      Check1(1).Value = 0
   Else
      If Check1(0).Value = 1 Then DataCombo1(15).Enabled = True
      If Check1(1).Value = 1 Then DataCombo1(16).Enabled = True
   End If
   DataCombo1(10).Enabled = mActivar
   txtIBNumeroInscripcion.Enabled = mActivar
   txtPorcentajeIBDirecto.Enabled = mActivar
   txtGrupoIIBB.Enabled = mActivar
   DTPicker1(1).Enabled = mActivar
   DTPicker1(2).Enabled = mActivar

End Sub
