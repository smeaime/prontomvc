  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>Select Rubros.Descripcion as [Rubro], SUM(df.Cantidad) as [Cantidad]
From DetalleFacturas df
Left Outer Join Facturas On Facturas.IdFactura=df.IdFactura
Left Outer Join Articulos On Articulos.IdArticulo=df.IdArticulo
Left Outer Join Rubros On Rubros.IdRubro=Articulos.IdRubro
Where IsNull(Facturas.Anulada,'NO')&lt;&gt;'SI' and Rubros.Descripcion is not null and YEAR(Facturas.FechaFactura)=2011
Group By Rubros.Descripcion
Order By [Cantidad] desc</CommandText>
      </Query>
      <Fields>
        <Field Name="Rubro">
          <DataField>Rubro</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Cantidad">
          <DataField>Cantidad</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
